{
  "name": "suid",
  "version": "1.0.0",
  "private": true,
  "description": "A port of Materia-UI (MUI) built with SolidJS.",
  "keywords": [
    "best_ecosystem",
    "components",
    "material",
    "material-design",
    "material-ui",
    "mui",
    "solid",
    "solid-js",
    "solid.js",
    "solidhack",
    "solidjs",
    "styled-component",
    "ui",
    "ux"
  ],
  "homepage": "https://suid.io",
  "bugs": {
    "url": "https://github.com/swordev/suid.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swordev/suid.git"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Juanra GM",
      "email": "juanrgm724@gmail.com",
      "url": "https://github.com/juanrgm"
    }
  ],
  "scripts": {
    "build": "pnpm scripts prebuild && pnpm build:ts && pnpm build:site",
    "build:site": "pnpm --filter @suid/site build && wspa compose -p @suid/site",
    "build:ts": "tsc --build tsconfig.build.json",
    "changeset": "changeset",
    "check": "pnpm format:check && pnpm lint",
    "clean": "wspa clean",
    "codemod": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm packages/codemod/src/bin.ts",
    "compose": "wspa compose",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postinstall": "wspa compose && pnpm scripts prebuild",
    "lint": "eslint --ignore-path .gitignore .",
    "prepare": "husky install",
    "release": "changeset publish",
    "scripts": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm scripts/index.ts",
    "start": "pnpm --filter @suid/site start",
    "test": "jest",
    "update": "ncu",
    "watch": "tsc --build tsconfig.build.json -w"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@changesets/changelog-github": "^0.4.6",
    "@changesets/cli": "^2.24.4",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@wspa/cli": "^0.5.1",
    "@wspa/config": "^0.2.2",
    "babel-jest": "^29.0.1",
    "babel-preset-solid": "^1.5.4",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-solid": "^0.7.1",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "npm-check-updates": "^16.0.6",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "prettier-plugin-sort-json": "^0.0.3",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.5.4",
    "solid-testing-library": "^0.3.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "info": "SUID is all a set of utils and components ported from MUI Core and much more.",
  "pnpm": {
    "overrides": {
      "@trivago/prettier-plugin-sort-imports>@babel/parser": "7.17.9"
    }
  },
  "x-wspa": {
    "extends": "@wspa/config/auto",
    "pkgManifest": {
      "bugs": {
        "url": "https://github.com/swordev/suid/issues"
      },
      "contributors": [
        {
          "name": "Juanra GM",
          "email": "juanrgm724@gmail.com",
          "url": "https://github.com/juanrgm"
        }
      ],
      "files": [
        "**/*.js",
        "**/*.jsx",
        "**/*.d.ts"
      ],
      "homepage": "https://suid.io",
      "keywords": [
        "best_ecosystem",
        "components",
        "material-design",
        "material-ui",
        "material",
        "mui",
        "solid-js",
        "solid.js",
        "solid",
        "solidhack",
        "solidjs",
        "styled-component",
        "ui",
        "ux"
      ],
      "license": "MIT",
      "repository": {
        "type": "git",
        "url": "https://github.com/swordev/suid",
        "directory": "<directory>"
      }
    }
  }
}
